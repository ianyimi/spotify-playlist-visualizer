{
	"name": "next-saas-starter",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "next build",
		"check": "next lint && tsc --noEmit",
		"dev": "next dev --port=3001 --turbo",
		"format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
		"lint": "eslint",
		"lint:fix": "eslint --fix",
		"preview": "next build && next start",
		"secret:set": "sst secret set",
		"secret:set:dev": "sst secret set --stage dev",
		"secret:set:prod": "sst secret set --stage production",
		"secret:remove:dev": "sst secret remove --stage dev",
		"secret:remove:prod": "sst secret remove --stage production",
		"secret:list:dev": "sst secret list --stage dev",
		"secret:list:prod": "sst secret list --stage production",
		"sso": "aws sso login --sso-session=uifoundry",
		"start": "next start",
		"test": "playwright test --config=playwright.config.ts",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@convex-dev/better-auth": "^0.9.7",
		"@convex-dev/react-query": "0.0.0-alpha.11",
		"@daveyplate/better-auth-ui": "^3.2.8",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-slot": "^1.2.3",
		"@t3-oss/env-nextjs": "^0.12.0",
		"@tanstack/react-query": "^5.69.0",
		"better-auth": "1.3.27",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"convex": "^1.28.2",
		"graphql": "^16.10.0",
		"lucide-react": "^0.525.0",
		"next": "16.0.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"server-only": "^0.0.1",
		"sharp": "^0.34.1",
		"superjson": "^2.2.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@convex-dev/eslint-plugin": "^1.0.0",
		"@eslint/eslintrc": "^3.3.1",
		"@next/eslint-plugin-next": "^15.4.3",
		"@playwright/test": "^1.56.1",
		"@tailwindcss/postcss": "^4.0.15",
		"@types/node": "^20.14.10",
		"@types/react": "^19.0.0",
		"@types/react-dom": "^19.0.0",
		"babel-plugin-react-compiler": "19.1.0-rc.1",
		"eslint": "^9.23.0",
		"eslint-config-next": "^15.2.3",
		"eslint-plugin-import-x": "^4.16.1",
		"eslint-plugin-perfectionist": "^4.15.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"postcss": "^8.5.3",
		"prettier": "^3.5.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"tailwindcss": "^4.0.15",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.27.0"
	},
	"ct3aMetadata": {
		"initVersion": "7.39.3"
	},
	"packageManager": "pnpm@10.11.0",
	"pnpm": {
		"overrides": {}
	}
}
